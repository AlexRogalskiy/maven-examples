{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    },
    "birthDate": {
      "type": "string"
    },
    "contactEmail": {
      "type": "string"
    },
    "isMarried": {
      "type": "boolean"
    }
  },
  "required": [ "id", "firstName", "lastName" ]
}

